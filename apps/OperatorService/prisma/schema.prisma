datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Operator {
  bigoId         String?
  createdAt      DateTime                 @default(now())
  diamondsAmount Int
  id             String                   @id @default(auto())
  operatorRole   EnumOperatorOperatorRole
  organizationId String
  updatedAt      DateTime
  userName       String?
}

enum EnumOperatorOperatorRole {
  Employee
  Agent
}
